	<queryString language="SQL">
		<![CDATA[SELECT
     phieunhap.`MaPN` AS phieunhap_MaPN,
     phieunhap.`NgayNhap` AS phieunhap_NgayNhap,
     sanpham.`TenSP` AS sanpham_TenSP,
     sanpham.`DVT` AS sanpham_DVT,
     nhacungcap.`MaNCC` AS nhacungcap_MaNCC,
     nhacungcap.`TenNCC` AS nhacungcap_TenNCC,
     nhacungcap.`SDT` AS nhacungcap_SDT,
     nhacungcap.`DiaChi` AS nhacungcap_DiaChi,
     nhacungcap.`Email` AS nhacungcap_Email,
     dondathang.`MaDDH` AS dondathang_MaDDH,
     dondathang.`NgayDatHang` AS dondathang_NgayDatHang,
     ctpn.`MaSP` AS ctpn_MaSP,
     ctpn.`SoLuong` AS ctpn_SoLuong,
     ctpn.`DonGia` AS ctpn_DonGia
FROM
     `nhacungcap` nhacungcap INNER JOIN `sanpham` sanpham ON nhacungcap.`MaNCC` = sanpham.`MaNCC`
     INNER JOIN `dondathang` dondathang ON nhacungcap.`MaNCC` = dondathang.`MaNCC`
     INNER JOIN `phieunhap` phieunhap ON dondathang.`MaDDH` = phieunhap.`MaDDH`
     INNER JOIN `ctpn` ctpn ON phieunhap.`MaPN` = ctpn.`MaPN`
     AND sanpham.`MaSP` = ctpn.`MaSP`]]>
	</queryString>
